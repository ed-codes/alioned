body{font-family:open sans,sans-serif;color:#36364d}body .strong{color:#36364d}.title{font-family:poppins,sans-serif;font-weight:400;color:#36364d}.is-font-light{font-weight:300}.alioned-brand{font-family:poppins,sans-serif;font-weight:600;text-transform:uppercase;letter-spacing:-.03em}.text-l{font-size:1.125rem}.text-m{font-size:1rem}.text-s{font-size:.875rem}.ed-violet{color:#5c6ac4}.kristi-orange{color:#ff7075}.alioned-violet{color:#4e51a6}.social-links li{font-family:initial;display:inline-block;color:#4e51a6;background:#fff;width:1.8em;height:1.8em;line-height:1.9em;vertical-align:middle;text-align:center;font-size:1.25em;border-radius:100%;margin-right:1rem}.social-links li a{color:#4e51a6}.social-links li.instagram-adjust svg{font-size:1.1em;position:relative;top:1px}.section{padding-top:6rem;padding-bottom:6rem}@media(max-width:768px){.section{padding-top:3rem;padding-bottom:3rem}}.section h2.title{margin-bottom:3rem}.button.alioned-button{border-radius:5px;font-weight:700;font-size:1rem;min-width:150px;height:50px;transition:transform .2s ease-in-out}.button.alioned-button:hover{-webkit-transform:scale(1.05);-ms-transform:scale(1.05);transform:scale(1.05)}.button.alioned-full{background-color:#4e51a6;box-shadow:5px 3px 10px rgba(0,0,0,.15);color:#fff;border:none}.button.alioned-outlined{border-color:#4e51a6;border-width:2px;box-shadow:5px 3px 10px rgba(0,0,0,.15);color:#4e51a6}header.section{padding-top:3rem;padding-top:3rem}header.section .navbar-brand .navbar-item{display:block}header.section .subtitle{color:#bdbdbd;font-weight:600;font-size:2rem}section.intro{background-image:url(assets/alioned.png);background-repeat:no-repeat;background-size:100%;background-position:50% 0;padding-top:0}@media(max-width:1000px){section.intro{background-position:50% 20%}}section.intro .main-links{font-weight:700;margin-bottom:60px;font-family:poppins,sans-serif}section.intro .main-links a{text-decoration:underline}section.intro .together{margin-bottom:10px}section.intro .alioned-brand{margin-bottom:6rem}@media(max-width:700px){section.intro{background-image:url(assets/alioned-mobile.png);background-size:80%}section.intro .main-links{margin-bottom:280px}section.intro .alioned-brand{margin-bottom:2rem}}@media(max-width:500px){section.intro .main-links{margin-bottom:220px}}@media(min-width:1200px){section.intro{background-size:90%;background-position:50% 0}}@media(min-width:1500px){section.intro{background-size:contain}section.intro .alioned-brand+p{display:none}}section.services{position:relative}section.services .background{position:absolute;top:0;left:0;width:100%;height:45%;background-color:#dbdced}section.services .lead{margin-bottom:3rem}section.services .column{display:flex}section.services .service{background:#fff;text-align:center;padding:2rem;box-shadow:0 0 15px rgba(0,0,0,.2);border-radius:5px;display:flex;flex-direction:column;align-items:center;width:100%}section.services .service img{width:42px;margin-bottom:2rem}section.services .service h3{font-weight:700;margin-bottom:.5rem}section.services .service p{margin-bottom:2rem}section.services .service a{margin-top:auto;margin-bottom:.5rem}section.principles{position:relative}section.principles .background{position:absolute;top:0;right:0;width:90%;height:90%;background-color:#ededf6}@media(max-width:768px){section.principles .background{width:100%;height:95%}}section.principles .title{margin-left:8.33333%}section.principles blockquote{padding-left:2rem;padding-top:32px;font-style:italic;position:relative;text-indent:32px;font-size:1.125rem}section.principles blockquote:before{content:'';height:32px;width:32px;position:absolute;top:0;left:16px;background-image:url(assets/quotes.png);background-size:100%}section.principles blockquote cite{font-style:normal;display:block;text-align:right;margin-top:1rem}section.principles .principle{margin-bottom:3rem}section.principles .principle h3{font-weight:700;margin-bottom:1rem;margin-left:2rem}section.portfolio{padding-top:0}section.portfolio .image-container{position:relative}section.portfolio .image-container:hover .img-cover{opacity:1}section.portfolio .image-container img{display:block}section.portfolio .image-container .img-cover{opacity:0;transition:opacity .5s ease-in-out;position:absolute;top:0;bottom:0;left:0;right:0;background:rgba(0,0,0,.5)}section.portfolio .image-container .img-cover .text{color:#fff;position:absolute;top:50%;left:50%;transform:translate(-50%,-50%)}section.portfolio .image-container .img-cover .text .site-name{margin-bottom:.5rem}section.portfolio .image-container .img-cover .text .button{background:transparent;color:#fff;border-color:#fff}@media(max-width:768px){section.portfolio .image-container .img-cover{opacity:1}}section.contact{background:#4e51a6;color:#fff}section.contact a,section.contact .title{color:#fff}section.contact .social-links li a{color:#4e51a6}section.contact .contact-info .title{margin-bottom:3rem}section.contact .contact-info .title+p{margin-bottom:3rem}section.contact .contact-info .title+p a{font-weight:600}section.contact .contact-info .social .text-s{margin-bottom:.5rem}section.contact .contact-info .social li{display:inline-block}section.contact .contact-form{margin-top:calc(3rem + 35px)}section.contact .contact-form .input,section.contact .contact-form .textarea{box-shadow:0 0 15px rgba(0,0,0,.2);border-radius:5px}section.contact .contact-form input{height:3em}section.contact .contact-form .button{background-color:#fff;color:#4e51a6;font-weight:600}@media(max-width:768px){section.contact .contact-info .title+p{margin-bottom:2rem}section.contact .contact-info .email{margin-bottom:2rem}section.contact .contact-form{margin-top:2rem}}section.setup-intro{padding-top:0}section.setup-intro .blurb{margin-bottom:2rem}section.our-setup .setup-cat{margin-bottom:2rem;font-weight:600}section.our-setup li{margin-bottom:2rem}section.our-setup li p:first-of-type{font-weight:600}